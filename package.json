{
  "name": "ngneat",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "nx serve playground",
    "test": "nx test",
    "e2e": "nx e2e --headless",
    "postinstall": "ngcc --properties es2015 browser module main && node ./decorate-angular-cli.js",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "build:lib": "ng build dirty-check-forms --configuration production",
    "test:lib": "nx test dirty-check-forms",
    "release": "npm run version && npm run build:lib",
    "version": "standard-version --packageFiles \"libs/dirty-check-forms/package.json\" --bumpFiles \"libs/dirty-check-forms/package.json\""
  },
  "dependencies": {
    "@angular/animations": "13.2.6",
    "@angular/common": "13.2.6",
    "@angular/compiler": "13.2.6",
    "@angular/core": "13.2.6",
    "@angular/forms": "13.2.6",
    "@angular/platform-browser": "13.2.6",
    "@angular/platform-browser-dynamic": "13.2.6",
    "@angular/router": "13.2.6",
    "lodash-es": "^4.17.21",
    "ng-zorro-antd": "13.1.1",
    "rxjs": "6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.10",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.2.6",
    "@angular/compiler-cli": "13.2.6",
    "@angular/language-service": "13.2.6",
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-angular": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@nrwl/angular": "13.8.8",
    "@nrwl/cli": "13.8.8",
    "@nrwl/cypress": "13.8.8",
    "@nrwl/eslint-plugin-nx": "13.8.8",
    "@nrwl/jest": "13.8.8",
    "@nrwl/linter": "13.8.8",
    "@nrwl/tao": "13.8.8",
    "@nrwl/workspace": "13.8.8",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "all-contributors-cli": "6.14.0",
    "cross-env": "7.0.2",
    "cypress": "9.5.2",
    "dotenv": "10.0.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "git-cz": "^3.2.1",
    "husky": "4.2.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "lint-staged": "12.3.5",
    "ng-packagr": "13.2.1",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "2.5.1",
    "standard-version": "9.3.2",
    "ts-jest": "27.0.5",
    "ts-node": "9.1.1",
    "typescript": "4.5.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run hooks:pre-commit && lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write"
    ]
  }
}
