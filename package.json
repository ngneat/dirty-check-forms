{
  "name": "ngneat",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve --o --port=3000",
    "build": "ng build",
    "test": "nx test",
    "e2e": "nx e2e --headless",
    "postinstall": "ngcc --properties es2015 browser module main && node ./decorate-angular-cli.js",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "build:lib": "ng build dirty-check-forms --configuration production",
    "test:lib": "nx test dirty-check-forms",
    "copy": "cp README.md dist/libs/dirty-check-forms",
    "dirty-check:release": "npm run dirty-check:version && npm run build:lib && npm run copy",
    "dirty-check:version": "standard-version --packageFiles=libs/dirty-check-forms/package.json",
    "run-migrations": "nx migrate --run-migrations=migrations.json"
  },
  "dependencies": {
    "@angular/animations": "12.2.16",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/forms": "12.2.16",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "core-js": "3.6.4",
    "ng-zorro-antd": "12.1.1",
    "rxjs": "6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.16",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "12.2.16",
    "@angular/compiler-cli": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@nrwl/angular": "12.10.1",
    "@nrwl/cli": "12.10.1",
    "@nrwl/cypress": "12.10.1",
    "@nrwl/eslint-plugin-nx": "12.10.1",
    "@nrwl/jest": "12.10.1",
    "@nrwl/linter": "12.10.1",
    "@nrwl/tao": "12.10.1",
    "@nrwl/workspace": "12.10.1",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "all-contributors-cli": "6.14.0",
    "cross-env": "7.0.2",
    "cypress": "4.3.0",
    "dotenv": "10.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "git-cz": "^3.2.1",
    "husky": "4.2.3",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "lint-staged": "10.1.1",
    "ng-packagr": "12.2.7",
    "prettier": "2.5.1",
    "standard-version": "7.1.0",
    "ts-jest": "27.0.5",
    "ts-node": "9.1.1",
    "typescript": "4.3.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run hooks:pre-commit && lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
